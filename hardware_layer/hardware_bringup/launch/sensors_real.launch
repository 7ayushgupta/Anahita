<launch>

    <!-- Arguments -->
    <arg name="namespace" default="anahita"/>
    <arg name="env" default="real"/>

    <include file="$(find hardware_camera)/launch/camera_logitech.launch">
    </include>

    <include file="$(find hardware_arduino)/launch/hardware_arduino.launch"/>

    <include file="$(find teledyne_navigator)/launch/teledyne_navigator.launch">
        <arg name="baudrate" value="115200">
        <arg name="port" value="/dev/ttyUSB0">

    <include file="$(find xsens_driver)/launch/xsens_driver.launch"/>
</launch> 

</launch>
